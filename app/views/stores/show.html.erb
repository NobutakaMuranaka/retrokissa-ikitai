<% provide(:title, "#{@store.name}") %>
<div class="container">
  <div class="row">
    <h2 class="store-name"><%= @store.name %></h2>
    <h3 class="store-adress"><%= @store.adress %></h2>
    <h3 class="store-business_hours"><%= @store.business_hours %></h2>
    <h3 class="store-regular_holiday"><%= @store.regular_holiday %></h2>
    <h3 class="store-description"><%= @store.description %></h2>
    <h3 class="store-image"><%= @store.image %></h2>
  </div>
</div>